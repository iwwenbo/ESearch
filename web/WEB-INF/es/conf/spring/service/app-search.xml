<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd"
       default-lazy-init="true">

    <bean id="rxSearchService" class="com.es.service.impl.RxSearchServiceImpl">
    </bean>
    <bean id="searchUtil" class="com.es.util.SearchUtil"
          factory-method="getElasticClient">
        <constructor-arg index="0" value="iwwenbo"></constructor-arg>
        <constructor-arg index="1" value="192.168.1.100:9300"></constructor-arg>
    </bean>
</beans>